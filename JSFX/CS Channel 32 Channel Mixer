// https://forum.cockos.com/showthread.php?t=61733
// desc: volume
// slider1:1<0,1,0.01>Trim

// @slider
// adj1 = slider1;
// doseek=1;

// @block
// doseek ? (
// dadj=(adj1-adj1_s)/samplesblock;
// doseek=0;
// );(
// dadj=0;
// adj1_s=adj1;
// );

// @sample
// spl0=spl0*adj1_s;
// spl1=spl1*adj1_s;
// adj1_s+=dadj;


////////////////////////////////////////////////////////////////////////////////


@slider

adj1 = (2^(slider1/6));
adj2 = (2^(slider2/6));
adj3 = (2^(slider3/6));
adj4 = (2^(slider4/6));
adj5 = (2^(slider5/6));
adj6 = (2^(slider6/6));
adj7 = (2^(slider7/6));
adj8 = (2^(slider8/6));
adj9 = (2^(slider9/6));
adj10 = (2^(slider10/6));
adj11 = (2^(slider11/6));
adj12 = (2^(slider12/6));
adj13 = (2^(slider13/6));
adj14 = (2^(slider14/6));
adj15 = (2^(slider15/6));
adj16 = (2^(slider16/6));
adj17 = (2^(slider17/6));
adj18 = (2^(slider18/6));
adj19 = (2^(slider19/6));
adj20 = (2^(slider20/6));
adj21 = (2^(slider21/6));
adj22 = (2^(slider22/6));
adj23 = (2^(slider23/6));
adj24 = (2^(slider24/6));
adj25 = (2^(slider25/6));
adj26 = (2^(slider26/6));
adj27 = (2^(slider27/6));
adj28 = (2^(slider28/6));
adj29 = (2^(slider29/6));
adj30 = (2^(slider30/6));
adj31 = (2^(slider31/6));
adj32 = (2^(slider32/6));


////////////////////////////////////////////////////////////////////////////////

@sample
spl(0) = spl(0)*adj1;
spl(1) = spl(1)*adj2;
spl(2) = spl(2)*adj3;
spl(3) = spl(3)*adj4;
spl(4) = spl(4)*adj5;
spl(5) = spl(5)*adj6;
spl(6) = spl(6)*adj7;
